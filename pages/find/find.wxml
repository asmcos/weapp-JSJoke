<view class="page">

    <view wx:for="{{list}}" wx:for-item="item" wx:key="*this">
            <view class="graySpace"></view>
            <view class="item-avatar-xs" wx:for="{{item.list_tool}}" wx:for-item="i" data-url="{{i.url}}" bindtap="goPage" wx:key="">
                <image src="{{i.img}}" />
                <view style="padding-left:10px">
                    <text class="title">{{i.name}}</text>
                </view>
            </view>
    </view>

</view>